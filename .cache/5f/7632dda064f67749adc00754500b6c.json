sition: absolute;\r\n        right: -10px;\r\n        top: -10px;\r\n        background-color: #97aabd; \r\n        padding: 6px;\r\n        border-radius: 30px;\r\n        color: #f5f5f5;\r\n    }\r\n\r\n`\r\n\r\nconst BlogSnipContent = styled(\"div\")`\r\n    display: grid;\r\n    aligh-items: end;\r\n    grid-template-columns: 3fr 1fr;\r\n    padding: 10px 10px 5px 10px;;\r\n    h2 {\r\n        font-size: 1.8em;\r\n        grid-column: 1 / span 2;\r\n        font-weight: bold;\r\n        margin-bottom: 10px;\r\n    }\r\n    p {\r\n        grid-column: 1;\r\n    }\r\n    a {\r\n        background-color: #9E5A63;\r\n        border-radius: 4px;\r\n        padding: 6px 20px;\r\n        box-shadow: 3px 3px 30px -10px black;\r\n        display: inline-block;\r\n        grid-column: 2;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        width: 80px;\r\n        height: 30px;\r\n        text-decoration: none;\r\n        color: #f5f5f5;\r\n    }\r\n    div {\r\n        display: flex;\r\n        align-items: end;\r\n    }\r\n`;\r\n\r\nconst BlogSnip = ({id, title, content}) => {\r\n    return (\r\n        <BlogSnipBody>\r\n            <img src=\"./\"/>\r\n            <BlogSnipContent>\r\n                <h2>{title}</h2>\r\n                <p>{content}</p>\r\n                <div>\r\n                    <Link to={`/posts/${id}`}>READ</Link>\r\n                </div>\r\n            </BlogSnipContent>\r\n            <Link to={`/posts/edit/${id}`}><i className=\"fas fa-pen fa-1x\"></i></Link>\r\n        </BlogSnipBody>    \r\n    )\r\n}\r\n\r\nexport default BlogSnip;"},"lineCount":null}},"error":null,"hash":"8c47d0014fbab00ab3fbf72346c17b1f","cacheData":{"env":{}}}
inal":{"line":42,"column":15},"generated":{"line":110,"column":18}},{"source":"ts/components/CategoriesView.tsx","name":null,"original":{"line":42,"column":0},"generated":{"line":110,"column":32}}],"sources":{"ts/components/CategoriesView.tsx":"/*\r\nAUTHOR: Devin Davis\r\nDATE: January 2st, 2021\r\nFILE: CategoryView.tsx\r\n*/\r\n\r\nimport * as React from \"react\";\r\nimport {useState, useEffect} from \"react\";\r\nimport styled from \"@emotion/styled\";\r\nimport TextArea from \"./TextArea\";\r\nimport Category from \"../controllers/category_controller\";\r\n\r\n// COMPONENTS\r\nimport Card from \"./Card\";\r\nimport CreateBtn from \"./CreateBtn\";\r\n\r\n// STYLES \r\nconst Body = styled(\"section\")`\r\n\r\n`;\r\n\r\nconst CategoriesView = () => {\r\n    const [categories, setCategories] = useState([]);\r\n    \r\n    useEffect(() => {\r\n      Category.prototype.getAll()\r\n      .then(data => setCategories(data))\r\n    }, []);\r\n  \r\n    return (\r\n        <Body>\r\n            <CreateBtn link=\"/categories/create\" />\r\n            <TextArea title=\"Welcome to my Blog\" content=\"A collection of articles for techies, fitness junkies and more!\" />\r\n            {categories.map((cat) => {\r\n                let postCount = cat.posts.length;\r\n                return <Card key={cat._id} count={postCount} id={cat._id} name={cat.name} desc={cat.desc} img=\"/\"/>\r\n                })}\r\n        </Body>\r\n    )\r\n}\r\n\r\nexport default CategoriesView;"},"lineCount":null}},"error":null,"hash":"8598f8997e12882aa990b08829d13d58","cacheData":{"env":{}}}
justify-content: center;\r\n        align-items: center;\r\n        width: 80px;\r\n        height: 30px;\r\n        text-decoration: none;\r\n        color: #f5f5f5;\r\n    }\r\n    div {\r\n        display: flex;\r\n        align-items: end;\r\n    }\r\n`;\r\n\r\nconst PostSnip = ({id, title, content}) => {\r\n    const [isDeleted, setIsDeleted] = useState(false);\r\n\r\n    const handleDelete = () => {\r\n        setIsDeleted(true);\r\n        Post.prototype.delete(id)\r\n    }\r\n\r\n    if(!isDeleted) {\r\n        return (\r\n            <PostSnipBody>\r\n                <img src=\"./\"/>\r\n                <PostSnipContent>\r\n                    <h2>{title}</h2>\r\n                    <p>{content}</p>\r\n                    <div>\r\n                        <Link to={`/posts/${id}`}>READ</Link>\r\n                    </div>\r\n                </PostSnipContent>\r\n                <Link to={`/posts/edit/${id}`}><i className=\"fas fa-pen fa-1x\"></i></Link>\r\n                <Link onClick={handleDelete} to={`/categories`}><i className=\"far fa-trash-alt\"></i></Link>\r\n            </PostSnipBody>    \r\n        )\r\n    } else {\r\n        return null\r\n    }\r\n}\r\n\r\nexport default PostSnip;"},"lineCount":null}},"error":null,"hash":"04aea1a2af9ac2e0f471d1bd96144e1e","cacheData":{"env":{}}}
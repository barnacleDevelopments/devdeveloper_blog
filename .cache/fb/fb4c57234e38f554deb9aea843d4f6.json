nAUTHOR: Devin Davis\r\nDATE: January 2st, 2021\r\nFILE: BlogsView.tsx\r\n*/\r\n\r\nimport * as React from \"react\";\r\nimport {FunctionComponent, useState, useEffect} from \"react\";\r\nimport styled from \"@emotion/styled\";\r\nimport { useLocation, useParams } from \"react-router-dom\";\r\n//COMPONENTS\r\nimport BlogSnip from \"./BlogSnip\"\r\nimport CategoryTitle from \"./CategoryTitle\";\r\n\r\nconst Body = styled(\"section\")`\r\n\r\n`;\r\n\r\n\r\nconst BlogsView = () => {\r\n    const [blogs, setBlogs] = useState([]);\r\n    const url = useLocation();\r\n    const { title } = useParams();\r\n\r\n    useEffect(() => {\r\n        fetch(`http://localhost:3000${url.pathname}`)\r\n        .then(response => response.json())\r\n        .then(data => setBlogs(data))\r\n    }, [])\r\n\r\n    return (\r\n        <Body>\r\n             <CategoryTitle title={title}/>\r\n            {blogs.map(blog => {\r\n                return (\r\n                    <BlogSnip key={blog._id} id={blog._id} title={blog.title} content={blog.content} />\r\n                )\r\n            })}\r\n        </Body>\r\n    )\r\n}\r\n\r\nexport default BlogsView;"},"lineCount":null}},"error":null,"hash":"94f6b6c34d30a7336b770c6db17efc13","cacheData":{"env":{}}}